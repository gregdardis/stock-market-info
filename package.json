    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^3.1.3",
