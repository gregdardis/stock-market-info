  "license": "ISC",
  "scripts": {
    "build": "webpack",
    "clean": "rm -r node_modules && cd src/client/ && rm -r node_modules && cd ../server/ && rm -r node_modules && cd ../../",
    "dev": "webpack-dev-server",
    "dev:test": "npm-run-all --parallel dev test:watch",
    "postinstall": "cd src/client/ && npm i && cd ../server/ && npm i & cd ../../",
    "start": "babel-node src/server",
    "test": "mocha --compilers js:babel-core/register",
    "test:watch": "mocha --watch --compilers js:babel-core/register"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.1.0",
    "nodemon": "^1.14.6",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "dateformat": "^3.0.3",
    "numeral": "^2.0.6"
