{
  "name": "stock-market-info",
  "scripts": {
    "build": "webpack --config ./src/client/config/webpack.config.js",
    "dev": "webpack-dev-server --config ./src/client/config/webpack.config.js",
    "dev:test": "npm-run-all --parallel dev test:watch",
    "install": "npm install && cd src/client/ && npm install",
    "start": "nodemon src/server/server.js",
    "test": "mocha --compilers js:babel-core/register",
    "test:watch": "mocha --watch --compilers js:babel-core/register"
  },
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^4.1.0",
    "nodemon": "^1.14.6",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "express": "^4.16.2",
    "yahoo-finance": "^0.3.5"
  }
}
