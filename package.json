{
  "name": "stock-market-info",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gregdardis/stock-market-info.git"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/buildScripts/testSetup.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/constants/**",
      "!src/client/render.js",
      "!src/server/config/index.js"
    ],
    "testMatch": [
      "**/*.test.{js,jsx}"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "license": "ISC",
  "scripts": {
    "build": "webpack --config ./config/webpack.config.dev.js",
    "dev": "webpack-dev-server --config ./config/webpack.config.dev.js",
    "dev:test": "npm-run-all --parallel dev test:watch",
    "start": "babel-watch src/server",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "chai": "^4.1.2",
    "chai-enzyme": "1.0.0-beta.1",
    "chalk": "^2.4.1",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.11",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-react": "^7.11.1",
    "fetch-mock": "7.0.0-alpha.6",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "react-hot-loader": "^3.1.3",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.2.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3"
  },
  "dependencies": {
    "array-flatten": "^2.1.1",
    "async": "^2.6.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "classnames": "2.2.6",
    "dateformat": "^3.0.3",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "material-ui": "^0.20.2",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-addons-css-transition-group": "15.6.2",
    "react-dom": "^16.4.2",
    "react-fontawesome": "^1.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.2.6",
    "recharts": "^1.1.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "yahoo-finance": "^0.3.5"
  }
}
